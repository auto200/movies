    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@movies/eslint-config": "workspace:*",
    "@movies/tsconfig": "workspace:*",
    "@types/node": "18.11.9",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "zod": "3.22.4"
  }
