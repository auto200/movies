{
  "name": "@movies/shared",
  "version": "1.0.0",
  "exports": {
    "./communication": "./src/communication/index.ts",
    "./utils": "./src/utils/index.ts",
    "./searchEngine": "./src/searchEngine/index.ts"
  },
  "typesVersions": {
    "*": {
      "communication": [
        "./src/communication/index.ts"
      ],
      "utils": [
        "./src/utils/index.ts"
      ],
      "searchEngine": [
        "./src/searchEngine/index.ts"
      ]
    }
  },
  "scripts": {
